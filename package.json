{
  "name": "pomodoro",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "w-ts": "tsc --strict -w src/ts/script.ts --outDir dist/js/",
    "w-sass": "sass -w src/sass/style.scss dist/css/style.css",
    "min-js": "uglifyjs dist/js/script.js -o dist/js/script.min.js",
    "min-css": "uglifycss dist/css/style.css > dist/css/style.min.css",
    "min-all": "npm run min-js; npm run min-css",
    "t-ts": "tsc",
    "t-sass": "sass src/sass/style.scss dist/css/style.css",
    "tam-script": "npm run t-ts; npm run min-js",
    "tam-style": "npm run t-sass; npm run min-css",
    "ttt": "tsc src/ts/toggleMuteMusic.ts src/ts/changeOption.ts --outDir dist/js/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  }
}
